{
	"name": "otpl-js",
	"version": "1.1.7",
	"description": "A simple template system, write once run everywhere!",
	"main": "dist/otpl.js",
	"scripts": {
		"build": "node build/index.js",
		"test:run": "node test/test.js",
		"test:bundle": "otpl-bundler --source \"./test/files\" --web-root \"./test/\" --dest-dir \"./test/web\""
	},
	"keywords": [
		"otpl",
		"template",
		"otemplate"
	],
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/silassare/otpl-js.git"
	},
